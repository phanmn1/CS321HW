// Yet another example of a good program
// Using upper case letters to skirt the identifier
// issue. Common implementation of reserved keywords.
int count = 0;
boolean y=2,z=3,a,y;
boolean y=2,z,e,y, Int, Boolean, Float, float,Integer;
int fact(int n) {
  count = count + 1;
  if (n<1)
    return 1;
  else
    return n * fact(n-1);
}

int x = y;
int y = 1;

void main() {
  print fact(7);
  print fact(3);
  print fact(count);
}

